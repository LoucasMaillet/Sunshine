<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta lang="fr">
    <title><%= Data.company %>™</title>
    <link href="<%= WebRoot.Img.ico %>" rel="shortcut icon">

    <link href="<%= WebRoot.Styles.index %>" rel="stylesheet">
    <link href="<%= WebRoot.Styles.home %>" rel="stylesheet">
    <link href="<%= WebRoot.Styles.projects %>" rel="stylesheet">
    <link href="<%= WebRoot.Styles.readme %>" rel="stylesheet">
    
    <script src="<%= WebRoot.Js.index %>" async></script>
</head>

<body>
    <menu class="appear">
        <div>
            <img src="<%= WebRoot.Img.ico %>">
            <a href="#home">Home</a>
            <a href="#readme">Readme</a>
            <div>
                <button href="#projects" menuLink="projectsMenu">Projects</button>
                <div menu="projectsMenu">
                    <a href="#projects-first">Project 1</a>
                    <a href="#projects-two">Project 2</a>
                </div>
            </div>
        </div>
        <form>
            <input type="text" placeholder="Search..." menuLink="searchMenu" maxlength="20">

            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                <path
                    d="M10 18a7.952 7.952 0 0 0 4.897-1.688l4.396 4.396 1.414-1.414-4.396-4.396A7.952 7.952 0 0 0 18 10c0-4.411-3.589-8-8-8s-8 3.589-8 8 3.589 8 8 8zm0-14c3.309 0 6 2.691 6 6s-2.691 6-6 6-6-2.691-6-6 2.691-6 6-6z">
                </path>
            </svg>
            <div menu="searchMenu">
                <p>Nothing found...</p>
            </div>
        </form>
    </menu>
    <main>
        <section id="home">
            <%- include(Templates.home) %>
        </section>
        <section id="readme">
            <%- include(Templates.readme) %>
        </section>
        <section id="projects">
            <%- include(Templates.projects) %>
        </section>
    </main>
    <footer>
        <% if(Data.tags && Data.tags.length > 0) { %>
        <section>
            <h2>Tags</h2>
            <div>
                <%for (const tag of Data.tags) { %>
                    <span><%= tag %></span>
                <% } %>
            </div>
        </section>
        <% } %>
        <section>
            <h2>Links</h2>
            <a href="#readme">Readme</a>
            <a href="#faq">Faq & About</a>
            <div>
                <a href="<%= Data.link.github %>">
